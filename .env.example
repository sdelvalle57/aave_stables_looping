# Alchemy API Key for RPC endpoints
VITE_ALCHEMY_API_KEY=your_alchemy_api_key_here

# WalletConnect Project ID for RainbowKit
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Convex API base (used only on Ethereum for boosted APY in Curve provider)
# Example: https://www.convexfinance.com or https://api.convexfinance.com (adjust to the correct base)
VITE_CONVEX_API_BASE=https://api.convexfinance.com
# Curve API base (optional; used by runtime resolver to fill pool addresses)
VITE_CURVE_API_BASE=https://api.curve.fi